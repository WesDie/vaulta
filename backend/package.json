{
  "name": "vaulta-backend",
  "version": "1.0.0",
  "description": "Vaulta API Server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "migrate": "node dist/migrate.js",
    "migrate:exif": "tsx src/scripts/addRawExifColumn.ts",
    "scan": "tsx src/scripts/scanMedia.ts",
    "generate-thumbs": "tsx src/scripts/generateThumbnails.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^6.12.0",
    "fastify": "^4.24.3",
    "pg": "^8.11.3",
    "sharp": "^0.32.6",
    "exifr": "^7.1.3",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "media",
    "gallery",
    "fastify",
    "typescript"
  ],
  "author": "Vaulta",
  "license": "MIT"
}
